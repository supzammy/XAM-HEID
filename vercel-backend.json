{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GEMINI_MODEL": "gemini-2.5-flash",
    "ENABLE_GEMINI_AI": "true",
    "FALLBACK_TO_ML": "true"
  }
}
